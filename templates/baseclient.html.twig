<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="{{ asset('font/css/font-awesome.min.css') }}"  rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    <link rel="icon" href="{{ asset('icons/favicon.ico') }}">
    <script type="text/javascript" src="{{ asset('js/burger.js') }}" defer></script>

    <title>{% block title %}Client Parking-Rue-Du-Moulin{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

</head>
<body>
<header>
    <p id="burger" class="burger"></p>
    <h1><a href="{{ path('default') }}">Parking Rue Du Moulin</a></h1>
    <nav>
        <ul>
            <li><a href="{{ path('default') }}">Accueil</a></li>
            <li><a href="{{ path('app_client_compte') }}"><img alt="User" src="{{ asset('icons/user.svg') }}"></a></li>
            <li><a href="{{ path('app_logout') }}"><img alt="Logout" src="{{ asset('icons/logout.svg') }}"></a></li>
        </ul>
    </nav>
</header>
<main>
    {% block body %}{% endblock %}
</main>
</body>
</html>
